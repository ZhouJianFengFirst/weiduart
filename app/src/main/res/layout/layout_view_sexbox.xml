<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/dp_20"

    android:gravity="center"
    android:orientation="horizontal">
    <ImageView
        android:id="@+id/register_img_man"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_20"
        android:src="@drawable/log_icon_man_no" />

    <TextView
        android:id="@+id/man_tx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_4"
        android:text="@string/register_txt_man"
        android:textColor="@color/colorWhite" />

    <ImageView
        android:id="@+id/register_img_woman"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_20"
        android:src="@drawable/log_icon_woman_no" />

    <TextView
        android:id="@+id/woman_tx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_4"
        android:text="@string/register_txt_woman"
        android:textColor="@color/colorWhite" />
</LinearLayout>

